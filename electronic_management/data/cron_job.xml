        <record id="Employee_Bday" model="ir.cron">
            <field name="name">Employee: Happy Birthday</field>
            <field name="model_id" ref="model_electronic_employee"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.check_bday_emp()</field>
            <field name="interval_number">1</field>
